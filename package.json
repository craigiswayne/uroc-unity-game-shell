{
  "name": "fugaso",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile:pug && npm run compile:sass && npm run compile:sass:dev && minify:js",
    "compile:pug": "pug index.pug --pretty",
    "compile:sass": "sass TemplateData/shell.scss TemplateData/shell.min.css --no-source-map --style=compressed",
    "compile:sass:dev": "sass TemplateData/shell.scss TemplateData/shell.css",
    "minify:js": "uglifyjs TemplateData/shell.js --output TemplateData/shell.min.js",
    "watch:pug": "npm run compile:pug -- --watch",
    "watch:sass": "npm run compile:sass:dev -- --watch"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "pug": "^3.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.89.2",
    "uglify-js": "^3.19.3"
  }
}
